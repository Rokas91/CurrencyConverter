  :git => 'https://github.com/Alamofire/Alamofire.git',
  :commit => '5bd458ca09a57fb2999772745d6438585f5140a7'

  pod 'SwiftyJSON',
  :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git',
  :commit => 'e9e53b8076e3cc8eabcb6ea53d70395b136dad5e'

end
